{
  "schemaVersion": 1,
  "id": "interfaced",
  "version": "${version}",

  "name": "Interfaced",
  "description": "Simple improvements to Minecraft's GUIs and the way information is displayed!",
  "authors": [
    "Sydokiddo"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/interfaced",
    "issues": "https://github.com/Sydokiddo/interfaced/issues",
    "sources": "https://github.com/Sydokiddo/interfaced"
  },
  "license": "MIT",
  "icon": "assets/interfaced/icon.png",
  "accessWidener": "interfaced.accesswidener",
  "environment": "*",

  "entrypoints": {
    "main": [
      "net.sydokiddo.interfaced.Interfaced"
    ],
    "client": [
      "net.sydokiddo.interfaced.client.InterfacedClient"
    ],
    "modmenu": [
      "net.sydokiddo.interfaced.misc.config.ModMenuCompatibility"
    ]
  },
  "mixins": [
    "interfaced.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.16.3",
    "fabric-api": "*",
    "minecraft": ">=1.21.1",
    "java": ">=21",
    "chrysalis": ">=0.4.2"
  },
  "suggests": {
    "another-mod": "*"
  },
  "breaks": {},
  "custom": {
    "modmenu": {
      "links": {
        "gui.interfaced.wiki": "https://github.com/Sydokiddo/interfaced/wiki"
      }
    }
  }
}